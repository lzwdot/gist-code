{
  "name": "express-blog-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run watch-*\"",
    "build": "cross-env NODE_ENV=production tsc && tsc --project tsweb.json && webpack --config webpack.ts",
    "watch-ts": "tsc --project tsweb.json --watch",
    "watch-node": "ts-node-dev --files ./src/main.ts",
    "watch-webpack": "webpack --config webpack.ts --watch",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "lint-staged": {
    "./src/**/*.{jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./src/**/*.{scss,less,styl,css,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@popperjs/core": "^2.11.8",
    "@toast-ui/editor": "^3.2.2",
    "@yaireo/tagify": "^4.17.9",
    "admin-lte": "^4.0.0-alpha3",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0",
    "dayjs": "^1.11.7",
    "debug": "^2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^2.6.1",
    "express": "^5.0.0-beta.1",
    "http-errors": "^1.6.3",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.1",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "sharp": "^0.33.0",
    "tributejs": "^5.1.3",
    "typeorm": "^0.3.16",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.4",
    "@types/bootstrap": "^5.2.10",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.46",
    "@types/debug": "^4.1.7",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.20",
    "@types/http-errors": "^2.0.1",
    "@types/i18n": "^0.13.10",
    "@types/method-override": "0.0.35",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.9",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
    "@types/passport-github": "^1.1.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/yaireo__tagify": "^4.17.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
